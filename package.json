{
  "name": "inline-source-cli",
  "version": "1.2.0",
  "description": "CLI for inline-source",
  "main": "dist/index.js",
  "bin": {
    "inline-source": "./dist/index.js"
  },
  "scripts": {
    "build": "babel src -d dist -s",
    "prebuild": "mkdirp dist",
    "lint": "eslint src",
    "pretest": "mkdirp test/out",
    "test": "mocha --timeout 5000",
    "prepare": "npm run build",
    "release": "npm run build && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/developit/inline-source-cli.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "mkdirp": "^0.5.1",
    "mocha": "^6.2.0"
  },
  "bugs": {
    "url": "https://github.com/developit/inline-source-cli"
  },
  "homepage": "https://github.com/developit/inline-source-cli",
  "keywords": [
    "inline",
    "source",
    "cli",
    "inliner"
  ],
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "dependencies": {
    "inline-source": "^7.1.0",
    "yargs": "^14.2.0"
  }
}
